{
  "name": "webmarker-js",
  "version": "0.0.6",
  "description": "A library for marking web pages for Set-of-Mark (SoM) prompting with vision-language models.",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "test": "jest",
    "build": "node esbuild.config.js",
    "demo:start": "esbuild demo/index.js --bundle --servedir=demo --outdir=demo/dist --loader:.js=jsx",
    "demo:build": "esbuild demo/index.js --bundle --minify --outdir=demo/dist",
    "docs:dev": "cd docs && next",
    "docs:build": "cd docs && next build",
    "docs:start": "cd docs && next start",
    "test:playwright": "cd test/playwright && npx playwright test"
  },
  "author": "Reid Barber",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.3",
    "@playwright/test": "^1.46.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.5.1",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.23.1",
    "esbuild-plugin-d.ts": "^1.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^14.2.3",
    "nextra": "^2.13.4",
    "nextra-theme-docs": "^2.13.4",
    "postcss": "^8.4.38",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.5"
  }
}
